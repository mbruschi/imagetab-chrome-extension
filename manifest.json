{
    "manifest_version": 2,

    "name": "imagetab",
    "description": "This extension displays a random image in a new chrome tab",
    "version": "0.93",

    "chrome_url_overrides" : {
        "newtab": "imagetab.html"
    },

    "permissions": [
        "storage"
    ],

    "icons": {
        "128": "icon_s.png"
    },

    "homepage_url": "https://plus.google.com/u/0/102193050494175107477",

    "options_page": "options.html"
}